{
   "cli": {
      "version": ">= 16.17.3",
      "appVersionSource": "remote"
   },
   "build": {
      "development": {
         "developmentClient": true,
         "distribution": "internal"
      },
      "preview": {
         "distribution": "internal"
      },
      "production": {
         "ios": {
            "distribution": "store",
            "autoIncrement": true,
            "developmentClient": false
         },
         "android": {
            "distribution": "store",
            "autoIncrement": true
         }
      }
   },
   "submit": {
      "production": {}
   }
}
